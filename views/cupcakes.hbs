<table>
    <thead>
        <tr>
            <th>Add</th> 
            <th></th>
            {{#each data.[0]}}
            <th>{{@key}}</th>
            {{/each}}
        </tr>
    </thead>

    <tbody>
        {{#each data}}
        <tr>
            <td>Edit</td>
            <td>Delete</td>
            <td align="center">{{this.cupcakeID}}</td>
            <td>{{this.cakeFlavor}}</td>
            <td>{{checkNull this.cakeColor}}</td>
            <td>{{this.frostingFlavor}}</td>
            <td>{{checkNull this.frostingColor}}</td>
            <td align="right">{{this.price}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>